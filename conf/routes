# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index
GET     /presentations						controllers.Presentations.list
GET     /presentations/$id<[0-9]+>			controllers.Presentations.index(id:Long)
POST    /presentations						controllers.Presentations.create
POST    /presentations/$id<[0-9]+>/delete	controllers.Presentations.delete(id:Long)

GET     /tasks/$id<[0-9]+>			controllers.Tasks.index(id:Long)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
